@article{Nastase2006a,
 abstract = {The goal of this paper is to investigate and develop a fast and robust algorithm for the solution of high-order accurate discontinuous Galerkin discretizations of non-linear systems of conservation laws on unstructured grids. Herein we present the development of a spectral hp-multigrid method, where the coarse ``grid'' levels are constructed by reducing the order (p) of approximation of the discretization using hierarchical basis functions (p-multigrid), together with the traditional (h-multigrid) approach of constructing coarser grids with fewer elements. On each level we employ variants of the element-Jacobi scheme, where the Jacobian entries associated with each element are treated implicitly (i.e., inverted directly) and all other entries are treated explicitly. The methodology is developed for the two-dimensional non-linear Euler equations on unstructured grids, using both non-linear (FAS) and linear (CGC) multigrid schemes. Results are presented for the channel flow over a bump and a uniform flow over a four element airfoil. Current results demonstrate convergence rates which are independent of both order of accuracy (p) of the discretization and level of mesh resolution (h).},
 author = {Nastase, Cristian R. and Mavriplis, Dimitri J.},
 doi = {10.1016/j.jcp.2005.08.022},
 file = {ScienceDirect Full Text PDF:/Users/anilyildirim/Zotero/storage/S85CDHTZ/Nastase and Mavriplis - 2006 - High-order discontinuous Galerkin methods using an.pdf:application/pdf;ScienceDirect Snapshot:/Users/anilyildirim/Zotero/storage/S6DM8D6X/S0021999105003839.html:text/html},
 issn = {0021-9991},
 journal = {Journal of Computational Physics},
 keywords = {Compressible flow, Computational fluid dynamics, Discontinuous Galerkin finite element methods, Gas dynamics, High order methods, Multigrid methods, Numerical methods},
 month = {March},
 number = {1},
 pages = {330--357},
 title = {High-order discontinuous {Galerkin} methods using an hp-multigrid approach},
 url = {http://www.sciencedirect.com/science/article/pii/S0021999105003839},
 urldate = {2019-04-05},
