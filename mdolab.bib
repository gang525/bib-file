@article{Menter2015a,
 abstract = {A model for the prediction of laminar-turbulent transition processes was formulated. It is based on the LCTM (`Local Correlation-based Transition Modelling') concept, where experimental correlations are being integrated into standard convection-diffusion transport equations using local variables. The starting point for the model was the $\gamma$-Re$\theta$model already widely used in aerodynamics and turbomachinery CFD applications. Some of the deficiencies of the $\gamma$-Re$\theta$model, like the lack of Galilean invariance were removed. Furthermore, the Re$\theta$equation was avoided and the correlations for transition onset prediction have been significantly simplified. The model has been calibrated against a wide range of Falkner-Skan flows and has been applied to a variety of test cases.},
 author = {Menter, Florian R. and Smirnov, Pavel E. and Liu, Tao and Avancha, Ravikanth},
 day = {01},
 doi = {10.1007/s10494-015-9622-4},
 issn = {1573-1987},
