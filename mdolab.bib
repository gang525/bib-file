@inproceedings{Drela1993,
 abstract = {An interactive design and optimization framework for the development of multielement airfoils is presented. A zonal Euler/Boundary-layer flow solver is used as the underlying analysis method. The full Newton solution method of the solver permits effectively 'free" calculation of flowfield sen- sitivities to predefined airfoil element shape deformation modes, and also to rigid-body element translation and ro- tation modes. The sensitivity of any aerodynamic quantity of interest, including drag, to all the geometric parameters is thus obtained. Single-point and multi-point optimiza- tion and least-squares inverse techniques are constructed with the sensitivity information. Aerodynamic and geomet- ric constraints are also readily incorporated. The method's ability to economically incorporate many design variables into interactively-specified design and/or optimization prob lems makes it particularly effective in multielement airfoil development. Applications are presented for: 1) Multi-point optimiza- tion of a novel two-element transonic cruise airfoil with a thickness constraint, and 2) CLmax optimization of a four- element landing configuration airfoil.},
 address = {Irvine, CA},
 author = {Mark Drela},
 booktitle = {Proceedings of the AIAA/AHS/ASEE Aerospace Design Conference},
 doi = {10.2514/6.1993-969},
