@article{Li2019,
 abstract = {Surrogate-based optimization is criticized in high-dimensional cases because it cannot scale well with the input dimension. In order to overcome this issue, we adopt a snapshot active subspace method to reduce the input dimension. A smoothing operation of samples is used to reduce the demand for snapshots in the construction of active subspaces. This operation significantly reduces the computational cost on the one hand, and on the other hand, it leads to more feasible subspaces. We use a 90{\^a}{\OE}95{\%} energy coverage criterion to define the dimension of the subspace. With this criterion, the surrogate-based airfoil optimization in the active subspace is both efficient and effective. We also validate this optimization approach in an ONERA M6 wing optimization case with 220 shape variables. Compared with original surrogate-based optimization, the new approach reduces the computational time by 70{\%} and obtains a more practical design with a smaller drag.},
 author = {Li, Jichao and Cai, Jinsheng and Qu, Kun},
 doi = {10.1007/s00158-018-2073-5},
 journal = {Structural and Multidisciplinary Optimization},
 month = {February},
 number = {2},
 pages = {403--419},
 title = {Surrogate-based aerodynamic shape optimization with the active subspace method},
