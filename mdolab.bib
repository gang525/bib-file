 doi = {10.1007/s10589-015-9741-9},
 issn = {1573-2894},
 journal = {Computational Optimization and Applications},
 month = {Nov},
 number = {2},
 pages = {449--475},
 title = {Exploiting derivative-free local searches in {DIRECT}-type algorithms for global optimization},
 url = {https://doi.org/10.1007/s10589-015-9741-9},
 volume = {65},
 year = {2016}
}

